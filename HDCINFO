// AMMO ASSOCIATIONS
// -----------------

// Add a new ammo association entry
// - ammoName = ClassName of the ammo to check against when adding things that use it
//
// newAmmo => { "ammoName": "FourMilAmmo" }

// Add a new ammo association to the given ammo type
// - ammoName = Name of the ammo to associate
// - name     = ClassName of the thing to associate with the ammo
// 
// addAmmoUser => { "ammoName": "HDPistolAmmo", "name": "MyCoolPistol" }

// Remove an existing ammo association entry
// - ammoName = ClassName of the ammo to remove
//
// removeAmmo => { "ammoName": "SevenMilAmmo" }

// Remove all existing ammo associations
//
// removeAllAmmo => {}

// HDest Ammo Associations
newAmmo => { "ammoName": "BrontornisRound" }
newAmmo => { "ammoName": "FourMilAmmo" }
newAmmo => { "ammoName": "HD4mMag" }
newAmmo => { "ammoName": "HD7mClip" }
newAmmo => { "ammoName": "HD7mMag" }
newAmmo => { "ammoName": "HD9mMag15" }
newAmmo => { "ammoName": "HD9mMag30" }
newAmmo => { "ammoName": "HDBattery" }
newAmmo => { "ammoName": "HDPistolAmmo" }
newAmmo => { "ammoName": "HDRevolverAmmo" }
newAmmo => { "ammoName": "HDRocketAmmo" }
newAmmo => { "ammoName": "HDShellAmmo" }
newAmmo => { "ammoName": "HEATAmmo" }
newAmmo => { "ammoName": "SevenMilAmmo" }
newAmmo => { "ammoName": "SevenMilAmmoRecast" }



// AMMO BOX SPAWN POOL COMMANDS
// --------------------------------

// Add a new ammo to the whitelist
// - ammoName = Name of the ammo to add
// - allowed  = Used to determine if ammo should be removed from blacklist if true, otherwise added
//
// addAmmoBoxFilter => { "ammoName": "FourMilAmmo", "allowed": "true" }

// Add a new ammo to the whitelist
// - ammoName = Name of the ammo to add
//
// addAmmoBoxWhitelist => { "ammoName": "FourMilAmmo" }

// Remove an existing ammo from the whitelist
// - ammoName = Name of the ammo to remove
//
// removeAmmoBoxWhitelist => { "ammoName": "SevenMilAmmo" }

// Remove all existing ammo from whitelist
//
// clearAmmoBoxWhitelist => {}

// Add a new ammo to the blacklist
// - ammoName = Name of the ammo to add
//
// addAmmoBoxBlacklist => { "ammoName": "FourMilAmmo" }

// Remove an existing ammo from the blacklist
// - ammoName = Name of the ammo to remove
//
// removeAmmoBoxBlacklist => { "ammoName": "SevenMilAmmo" }

// Remove all existing ammo from blacklist
//
// clearAmmoBoxBlacklist => {}



// BACKPACK SPAWN POOL COMMANDS
// --------------------------------

// Add a new ammo to the whitelist
// - name = Name of the item to add
// - allowed  = Used to determine if item should be added to spawn pool if true, otherwise remove it
//
// addBPSpawnPoolFilter => { "name": "HDInvulnerabilitySphere", "allowed": "false" }

// Add a new item to the whitelist
// - name = Name of the item to add
//
// addBPSpawnPoolWhitelist => { "name": "HDBlursphere" }

// Remove an existing item from the whitelist
// - name = Name of the item to remove
//
// removeBPSpawnPoolWhitelist => { "name": "HDMegaSphere" }

// Remove all existing items from whitelist
//
// clearBPSpawnPoolWhitelist => {}

// Add a new item to the blacklist
// - name = Name of the item to add
//
// addBPSpawnPoolBlacklist => { "name": "HDBlursphere" }

// Remove an existing item from the blacklist
// - name = Name of the item to remove
//
// removeBPSpawnPoolBlacklist => { "name": "HDMegaSphere" }

// Remove all existing items from blacklist
//
// clearBPSpawnPoolBlacklist => {}
