[en default]

// Logging Prefixes
HDC_LOG_PREFIX_ERROR = "[ ‼ ] ";
HDC_LOG_PREFIX_WARN  = "[ ! ] ";
HDC_LOG_PREFIX_INFO  = "[ i ] ";
HDC_LOG_PREFIX_DEBUG = "[ ? ] ";
HDC_LOG_PREFIX_TRACE = "[ > ] ";
HDC_LOG_PREFIX_LOG   = "[ ✔ ] ";

// Logging Colors
HDC_LOG_COLOR_ERROR  = "\c[red]";
HDC_LOG_COLOR_WARN   = "\c[gold]";
HDC_LOG_COLOR_INFO   = "\c[ice]";
HDC_LOG_COLOR_DEBUG  = "\c[teal]";
HDC_LOG_COLOR_TRACE  = "\c[darkgray]";
HDC_LOG_COLOR_LOG    = "\c-";
